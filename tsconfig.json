{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "src",
		"types": [
			"vite-svg-loader"
		],
		"typeRoots": [
			"node_modules/@types",
			"src/typings"
		]
	},
	"include": [
		".eslintrc.js",
		"*.config.js",
		"*.config.ts",
		"src/*.vue",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.vue"
]
}
